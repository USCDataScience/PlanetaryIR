            "target_mention",
            "targets_chemcam_mer"
          ],
          "output_relation": "target_label",
          "style": "cmd_extractor"
        },
        "ext_target_mention_by_map_target_mention": {
          "dependencies": [
            "ext_sentences_by_nlp_markup"
          ],
