          "input_batch_size": "100000",
          "input_relations": [
            "target_mention",
            "sentences"
          ],
          "output_relation": "target_feature",
          "parallelism": "1",
          "style": "tsv_extractor",
          "udf": "\"$DEEPDIVE_APP\"/udf/extract_target_features.py"
        },
        "ext_target_label": {
          "cmd": "\n\n\t# TODO use temporary table\n\tdeepdive create table \"target_label\"\n\tdeepdive sql 'INSERT INTO target_label SELECT R0.mention_id AS \"target_mention.R0.mention_id\", R0.mention_text AS \"target_mention.R0.mention_text\", 1 AS column_2, '\\''from_Chemcam'\\'' AS column_3\nFROM target_mention R0, targets_chemcam_mer R1\n        WHERE lower(R1.target_name) = lower(R0.mention_text)'\n\t# TODO rename temporary table to replace output_relation\n\t\n        ",
